var x:vec3;
%data intlist = #NIL
%     		  | #CONS of int * intlist;
data vec3 = #nil1
		  | #nil2
		  | #vec3 of int * int * int;
begin
%x := #CONS(5,#NIL);
%y := #CONS(4,x);
x := #vec3(4,5,6);
case x of {
	#vec3(x,y,z) => {
		print x;
		print y;
		print z;
	}
	| #nil2 => {
		print "n";
		print 1;		
	}
	| #nil1 => {
		print "n";
		print 2;		
	}
};
/*case y of {
	  #NIL => print "N"
	| #CONS(x,xs) => { 
		print "C";
		print x;
	  }
};
*/
end